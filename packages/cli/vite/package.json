{
  "name": "@hattip/vite",
  "version": "0.0.33",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "rimraf dist && tsup"
  },
  "description": "Vite plugin and CLI wrapper for Hattip",
  "bin": {
    "hattip": "./hattip.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./node-adapter": "./dist/node-adapter.js",
    "./node-loader": "./node-loader.mjs",
    "./suppress-loader-warnings": "./suppress-loader-warnings.cjs"
  },
  "typesVersions": {
    "*": {
      "vite-dev-server": [
        "./vite-dev-server.d.ts"
      ],
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "hattip.js"
  ],
  "peerDependencies": {
    "vite": ">= 3"
  },
  "devDependencies": {
    "rimraf": "^4.4.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  },
  "dependencies": {
    "@hattip/adapter-node": "workspace:*",
    "@vavite/connect": "^1.5.3",
    "@vavite/multibuild": "^1.5.3",
    "@vavite/node-loader": "^1.6.1",
    "cac": "^6.7.14",
    "import-meta-resolve": "^2.2.1",
    "picocolors": "^1.0.0",
    "resolve.exports": "^2.0.0"
  }
}
